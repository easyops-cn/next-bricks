{
  "name": "@next-bricks/v2-adapter",
  "version": "0.3.18",
  "homepage": "https://github.com/easyops-cn/next-bricks/tree/master/bricks/v2-adapter",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-bricks.git"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "build": "npm run build:main && npm run build:types",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@next-core/easyops-runtime": "^0.6.55",
    "@next-core/element": "^1.1.6",
    "@next-core/http": "^1.1.5",
    "@next-core/i18n": "^1.0.32",
    "@next-core/loader": "^1.4.4",
    "@next-core/react-element": "^1.0.16",
    "@next-core/runtime": "^1.25.7",
    "@next-core/theme": "^1.4.2",
    "@next-core/utils": "^1.6.4",
    "history": "^4.10.1",
    "js-yaml": "^3.14.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@next-core/brick-dll-v3": "^0.7.16",
    "@next-core/build-next-bricks": "^1.16.0",
    "@next-core/styles-v3": "^0.3.3",
    "@next-core/test-next": "^1.0.11",
    "@next-dll/ace": "^2.0.700",
    "@next-dll/d3": "^2.0.79",
    "@next-dll/echarts": "^2.0.80",
    "@next-dll/editor-bricks-helper": "^0.38.45",
    "@next-dll/react-dnd": "^0.1.638",
    "copy-webpack-plugin": "^11.0.0"
  }
}
