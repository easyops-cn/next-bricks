{
  "name": "@next-bricks/basic",
  "version": "1.45.3",
  "homepage": "https://github.com/easyops-cn/next-bricks/tree/master/bricks/basic",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-bricks.git",
    "directory": "bricks/basic"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types",
    "docs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./dist/manifest.json": "./dist/manifest.json",
    "./data-providers/*": {
      "types": "./dist-types/data-providers/*.d.ts"
    },
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "build": "npm run build:main && npm run build:types",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json",
    "build:manifest": "cross-env NODE_ENV=production build-next-bricks --manifest-only",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@next-api-sdk/micro-app-standalone-sdk": "^1.1.1",
    "@next-api-sdk/user-service-sdk": "^1.3.0",
    "@next-core/element": "^1.2.13",
    "@next-core/http": "^1.2.8",
    "@next-core/i18n": "^1.0.68",
    "@next-core/react-element": "^1.0.32",
    "@next-core/react-runtime": "^1.6.44",
    "@next-core/runtime": "^1.55.3",
    "@next-core/theme": "^1.5.4",
    "@next-core/types": "^1.13.0",
    "@next-shared/general": "^0.9.8",
    "@next-shared/story": "^0.0.2",
    "broadcast-channel": "^7.0.0",
    "classnames": "^2.3.2",
    "dompurify": "^3.1.6",
    "lodash": "^4.17.21",
    "react": "0.0.0-experimental-ee8509801-20230117"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.23.3",
    "@next-core/test-next": "^1.1.7"
  },
  "peerDependencies": {
    "@next-bricks/icons": "*"
  }
}
