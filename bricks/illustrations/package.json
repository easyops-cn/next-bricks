{
  "name": "@next-bricks/illustrations",
  "version": "1.5.0",
  "homepage": "https://github.com/easyops-cn/next-bricks/tree/master/bricks/illustrations",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-bricks.git",
    "directory": "bricks/illustrations"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types",
    "docs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./dist/manifest.json": "./dist/manifest.json",
    "./data-providers/*": {
      "types": "./dist-types/data-providers/*.d.ts"
    },
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    },
    "./jsx.d.ts": "./dist-types/jsx.d.ts"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist",
          "{projectRoot}/dist-types",
          "{projectRoot}/.pkgbuild",
          "{projectRoot}/deploy",
          "{projectRoot}/src/generated"
        ]
      },
      "build:types": {
        "outputs": [
          "{projectRoot}/dist-types",
          "{projectRoot}/src/generated"
        ]
      }
    }
  },
  "scripts": {
    "prestart": "node ./scripts/pre-build.js",
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "prebuild": "node ./scripts/pre-build.js",
    "prebuild:types": "node ./scripts/pre-build.js",
    "build": "npm run build:main && npm run build:types",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json && cp src/jsx.d.ts dist-types/jsx.d.ts",
    "build:manifest": "cross-env NODE_ENV=production build-next-bricks --manifest-only",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@next-core/element": "^1.2.19",
    "@next-core/illustrations": "^0.11.34",
    "@next-core/react-element": "^1.0.38",
    "@next-core/react-runtime": "^1.7.38",
    "@next-core/types": "^1.17.0",
    "@next-core/utils": "^1.8.6",
    "lodash": "^4.17.21",
    "react": "0.0.0-experimental-ee8509801-20230117"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.24.0",
    "@next-core/test-next": "^2.0.1"
  },
  "peerDependencies": {
    "@next-bricks/icons": "*"
  }
}
