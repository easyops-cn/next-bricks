{
  "name": "@next-bricks/diagram",
  "version": "0.54.3",
  "homepage": "https://github.com/easyops-cn/next-bricks/tree/master/bricks/diagram",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-bricks.git",
    "directory": "bricks/diagram"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types",
    "docs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./dist/manifest.json": "./dist/manifest.json",
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "build": "npm run build:main && npm run build:types",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json",
    "build:manifest": "cross-env NODE_ENV=production build-next-bricks --manifest-only",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.0",
    "@dagrejs/dagre": "^1.1.3",
    "@next-api-sdk/cmdb-sdk": "^1.1.1",
    "@next-core/element": "^1.2.16-beta.0",
    "@next-core/i18n": "^1.0.73-beta.1",
    "@next-core/react-element": "^2.0.0-beta.1",
    "@next-core/react-runtime": "^2.0.0-beta.1",
    "@next-core/runtime": "^1.59.1-beta.1",
    "@next-core/theme": "^1.5.4",
    "@next-core/utils": "^1.7.31-beta.0",
    "antd": "^5.23.0",
    "classnames": "^2.3.2",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-zoom": "^3.0.0",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.23.8",
    "@next-core/test-next": "^1.1.8-beta.0"
  },
  "peerDependencies": {
    "@next-bricks/basic": "*"
  }
}
